{
    "name": "Your Search",
    "description": "Your own search field.",
    "version": "1.0",
    "permissions": [
        "storage",
        "unlimitedStorage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "19": "icons/icon32.png"
        },
        "default_title": "Your Search",
        "default_popup": "popup.html"
    },
    "omnibox": {
        "keyword": "ys"
    },
    "manifest_version": 2,
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "options_page": "options.html"
}